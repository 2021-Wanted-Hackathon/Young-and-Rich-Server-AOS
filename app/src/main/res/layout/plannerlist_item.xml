<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/white">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/plannerList_item_const1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:background="@drawable/background_view4"
        android:layout_marginBottom="10dp"
        android:elevation="5dp">

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/plannerList_item_txt1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="쇼핑"
            android:textSize="15dp"
            android:textColor="@color/black"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginStart="15dp"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="15dp"/>

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/plannerList_item_txt2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="500,000원 / "
            android:textColor="@color/black"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@id/plannerList_item_txt3"
            />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/plannerList_item_txt3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="350,000원"
            android:textColor="@color/blackForText"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="15dp"/>
    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>